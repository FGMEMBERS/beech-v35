<?xml version="1.0"?>
<PropertyList>
    <path>beech35.ac</path>

    <model>
        <usage>interior</usage>
        <path>Models/a35-interior.xml</path>
    </model>

    <specular>
        <red-prop>specular/red</red-prop>
        <green-prop>specular/green</green-prop>
        <blue-prop>specular/blue</blue-prop>
    </specular>

    <emission>
        <red-prop>emission/red</red-prop>
        <green-prop>emission/green</green-prop>
        <blue-prop>emission/blue</blue-prop>
    </emission>

    <!-- Livery -->
    <nasal>
        <load>
            var livery_update = aircraft.livery_update.new("Aircraft/beech-v35/Models/Liveries");
        </load>

        <unload>
            livery_update.stop();
        </unload>
    </nasal>

    <animation>
        <type>material</type>
        <texture-prop>texture</texture-prop>
        <texture>Models/Liveries/redstripe.png</texture>
        <property-base>sim/model/livery</property-base>

        <object-name>fuselage</object-name>
        <object-name>wing</object-name>
        <object-name>rightaileron</object-name>
        <object-name>rightflap</object-name>
        <object-name>leftaileron</object-name>
        <object-name>leftflap</object-name>
