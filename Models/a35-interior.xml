    <!-- gear indicator -->
    <animation>
        <type>material</type>
        <object-name>gearlight</object-name>
        <object-name>gearglass</object-name>
        <condition>
            <equals>
                <property>controls/gear/gear-down</property>
                <value>true</value>
            </equals>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
        </emission>
    </animation>
    <animation>
        <type>pick</type>
        <object-name>gearlight</object-name>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <label>Gear Status: %s</label>
                <tooltip-id>gearindicator</tooltip-id>
                <property>controls/gear/gear-down</property>
                <mapping>nasal</mapping>
                <script>
                    if(arg[0]) {
                        return 'Down';
                    } else {
                        return 'Up';
                    }
                </script>
            </binding>
        </hovered>
    </animation>

    <!-- cowl flaps -->
    <animation>
        <type>translate</type>
        <object-name>cowlflaps</object-name>
        <property>controls/engines/engine/cowl-flaps-norm</property>
        <factor>.01</factor>
        <axis>
            <x>1.0</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>pick</type>
        <object-name>cowlflaps</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>controls/engines/engine/cowl-flaps-norm</property>
                <value type="int">1</value>
                <value type="int">0</value>
            </binding>
        </action>
        <hovered>
            <binding>
                <command>set-tooltip</command>
                <label>Cowl Flaps: %s</label>
                <tooltip-id>cowl-flaps</tooltip-id>
                <property>controls/engines/engine/cowl-flaps-norm</property>
                <mapping>nasal</mapping>
                <script>
                    var pos = ['Closed', 'Open'];
                    return pos[arg[0]];
                </script>
            </binding>
        </hovered>
    </animation>

