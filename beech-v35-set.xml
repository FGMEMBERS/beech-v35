<?xml version="1.0"?>
<PropertyList>
    <sim>
        <description>Beech V35 Bonanza</description>
        <aircraft-version>0.0.1</aircraft-version>
        <author>flyingfisch</author>
        <status>development</status>
        <rating>
            <FDM type="int">1</FDM>
            <systems type="int">0</systems>
            <cockpit type="int">2</cockpit>
            <model type="int">3</model>
        </rating>

        <flight-model>yasim</flight-model>
        <aero>beech-v35</aero>

        <!-- x/y/z == right/up/back -->
        <view n="0">
            <internal archive="y">true</internal>
            <config>
                <x-offset-m archive="y">-.279348</x-offset-m> 
                <y-offset-m archive="y">.495983</y-offset-m>   
                <z-offset-m archive="y">2.4466</z-offset-m>   
                <pitch-offset-deg>-15</pitch-offset-deg>
            </config>
        </view>

        <autopilot>
            <path>Systems/vtail.xml</path>
        </autopilot>

        <!--<startup>
        <splash-texture>Aircraft/pa28-236/Splashes/splash.png</splash-texture>
        <splash-title>Beech V35 Bonanza</splash-title>
        </startup>-->

        <model>
            <path>Aircraft/beech-v35/Models/beech-v35.xml</path>
        </model>

        <!--<sound>
        <path>Aircraft/pa28-236/pa28-236-sound.xml</path>
        </sound>-->

        <!--autopilot>
        <path></path>
        </autopilot-->

        <!--<flaps>
        <setting>0.00</setting>
        <setting>0.5</setting>
        <setting>1.00</setting>
        </flaps>-->

        <!-- Menubar -->
        <!--<menubar include="Dialogs/pa28-236-menu.xml" />-->

        <!-- Weights -->
        <weight n="0">
            <name>Pilot</name>
            <weight-lb>170</weight-lb>
            <max-lb>300</max-lb>
        </weight>
        <weight n="1">
            <name>Copilot</name>
            <weight-lb>0</weight-lb>
            <max-lb>300</max-lb>
        </weight>
        <weight n="2">
            <name>Left Passenger</name>
            <weight-lb>0</weight-lb>
            <max-lb>300</max-lb>
        </weight>
        <weight n="3">
            <name>Right Passenger</name>
            <weight-lb>0</weight-lb>
            <max-lb>300</max-lb>
        </weight>
        <weight n="4">
            <name>Baggage</name>
            <weight-lb>200</weight-lb>
            <max-lb>200</max-lb>
        </weight>

    </sim>

